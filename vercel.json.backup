{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_STRIPE_PUBLISHABLE_KEY": "@stripe-publishable-key",
    "VITE_STRIPE_SECRET_KEY": "@stripe-secret-key",
    "VITE_STRIPE_STARTER_PRICE_ID": "@stripe-starter-price-id",
    "VITE_STRIPE_GROWTH_PRICE_ID": "@stripe-growth-price-id",
    "VITE_STRIPE_PROFESSIONAL_PRICE_ID": "@stripe-professional-price-id",
    "STRIPE_WEBHOOK_SECRET": "@stripe-webhook-secret",
    "VITE_FIREBASE_PROJECT_ID": "@firebase-project-id",
    "FIREBASE_CLIENT_EMAIL": "@firebase-client-email",
    "FIREBASE_PRIVATE_KEY": "@firebase-private-key"
  }
}